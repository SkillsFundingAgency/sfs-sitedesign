<script type="text/javascript">
    function dualAuthOptions() {
      if(document.getElementById('sign-in-dfe').checked) {
          document.getElementById('exampleRadioLink').onclick = location.href='login-dfe-sens';
      }
      else if(document.getElementById('sign-in-idams').checked) {
          document.getElementById('exampleRadioLink').onclick = location.href='login-idams-sens';
      }
      else {
        $(".govuk-form-group").addClass("govuk-form-group--error");
        $("#passport-issued-error").removeClass("hide");
      }
    }
  </script>

